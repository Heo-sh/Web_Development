<%@page import="vo.Person"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<%
	Person vo = new Person();
	
	vo.setName("홍길동");
	vo.setAge(20);
	
	request.setAttribute("vo", vo);
%>

<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
	</head>
	<body>
		이름: ${vo.name}<br>
	</body>
</html>